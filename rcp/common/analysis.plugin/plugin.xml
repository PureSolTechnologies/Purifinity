<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.puresol.coding.client.common.analysis.AnalysisAPIStartup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.puresol.coding.client.common.analysis.perspectives.AnalysisPerspective"
            id="com.puresol.coding.client.common.analysis.perspectives.AnalysisPerspective"
            name="Analysis">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.puresol.coding.client.common.analysis.editors.FileAnalysisEditor"
            default="false"
            id="com.puresol.coding.client.common.analysis.editors.FileAnalysisEditor"
            name="File Analysis">
      </editor>
      <editor
            class="com.puresol.coding.client.common.analysis.editors.DirectoryAnalysisEditor"
            default="false"
            id="com.puresol.coding.client.common.analysis.editors.DirectoryAnalysisEditor"
            name="Directory Analysis">
      </editor>
      <editor
            class="com.puresol.coding.client.common.analysis.editors.NotAnalyzedEditor"
            default="false"
            id="com.puresol.coding.client.common.analysis.editors.NotAnalyzedEditor"
            name="Not Analyzed">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.puresol.coding.client.common.analysis.preferences.NewAnalysisPreferencePage"
            id="com.puresol.coding.client.common.analysis.preferences.NewAnalysisPreferencePage"
            name="New Analysis">
      </page>
      <page
            class="com.puresol.coding.client.common.analysis.preferences.CodeAnalysisPreferencePage"
            id="com.puresol.coding.client.common.analysis.preferences.CodeAnalysisPreferencePage"
            name="Code Analysis">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.puresol.coding.client.common.analysis.handlers.NewAnalysisProjectHandler"
            id="com.puresol.coding.client.common.analysis.handlers.NewAnalysisProjectHandler"
            name="New Analysis">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.puresol.coding.client.common.analysis.AnalysisCategory"
            name="Code Analysis">
      </category>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AnalysisProjectsView"
            id="com.puresol.coding.client.common.analysis.views.AnalysisProjectsView"
            name="Analysis Projects"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AnalysisRunContentView"
            id="com.puresol.coding.client.common.analysis.views.AnalysisRunContentView"
            name="Analysis Run Content"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AnalysisRunsView"
            id="com.puresol.coding.client.common.analysis.views.AnalysisRunsView"
            name="Analysis Runs"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AnalysisReportView"
            id="com.puresol.coding.client.common.analysis.views.AnalysisReportView"
            name="Analysis Report"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AnalysisStoreView"
            id="com.puresol.coding.client.common.analysis.views.AnalysisStoreView"
            name="Analysis Store"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.AvailableLanguagesView"
            id="com.puresol.coding.client.common.analysis.views.AvailableLanguagesView"
            name="Available Languages"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.GrammarView"
            id="com.puresol.coding.client.common.analysis.views.GrammarView"
            name="Grammar Viewer"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.client.common.analysis.AnalysisCategory"
            class="com.puresol.coding.client.common.analysis.views.ParserTreeView"
            id="com.puresol.coding.client.common.analysis.views.ParserTreeView"
            name="Parser Tree Viewer"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.puresol.coding.client.common.analysis.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
