<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.puresol.coding.client.SourceCodeAnalysis">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Analysis Perspective"
            class="com.puresol.coding.client.perspectives.AnalysisPerspective"
            id="com.puresol.coding.client.AnalysisPerspective">
      </perspective>
      <perspective
            class="com.puresol.coding.client.perspectives.EvaluationPerspective"
            id="com.puresol.coding.client.EvaluationPerspective"
            name="Evaluation Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="file"
               label="File"
               mnemonic="F">
            <command
                  commandId="com.puresol.ui.file.newAnalysis"
                  id="newAnalysis"
                  label="New Analysis..."
                  mnemonic="N"
                  style="push">
            </command>
            <separator
                  name="coding.client.plugin.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.file.print"
                  id="print"
                  label="Print..."
                  mnemonic="P"
                  style="push">
            </command>
            <separator
                  name="coding.client.plugin.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="com.puresol.ui.file.switchWorkspace"
                  id="switchWorkspace"
                  label="Switch Workspace..."
                  mnemonic="W"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.restartWorkbench"
                  id="restart"
                  label="Restart"
                  style="push">
            </command>
            <separator
                  name="RichClient.Repository.separator5"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.file.export"
                  id="export"
                  label="Export..."
                  mnemonic="E"
                  style="push">
            </command>
            <separator
                  name="RichClient.Repository.separator6"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.file.exit"
                  id="Exit"
                  label="Exit"
                  mnemonic="x"
                  style="push">
            </command>
         </menu>
         <menu
               id="options"
               label="Options"
               mnemonic="O">
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  id="preferences"
                  label="Preferences..."
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
         <menu
               id="window"
               label="Window"
               mnemonic="W">
            <command
                  commandId="org.eclipse.ui.window.newWindow"
                  id="newWindow"
                  label="New Window"
                  mnemonic="N"
                  style="push">
            </command>
            <separator
                  name="RichClient.Repository.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.views.showView"
                  id="showView"
                  label="Show View..."
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.perspectives.showPerspective"
                  id="showPerspective"
                  label="Show Perspective..."
                  mnemonic="P"
                  style="push">
            </command>
            <separator
                  name="RichClient.Repository.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.window.customizePerspective"
                  id="customizePerspectiveAs"
                  label="Customize Perspective..."
                  mnemonic="A"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.savePerspective"
                  id="savePerspective"
                  label="Save Perspective As..."
                  mnemonic="A"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.resetPerspective"
                  id="resetPerspective"
                  label="Reset Perspective..."
                  mnemonic="R"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.closePerspective"
                  id="closePerspective"
                  label="Close Perspective"
                  mnemonic="C"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.closeAllPerspectives"
                  id="closeAllPerspectives"
                  label="Close All Perspectives"
                  mnemonic="l"
                  style="push">
            </command>
         </menu>
         <menu
               id="help"
               label="Help"
               mnemonic="H">
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  id="about"
                  label="About Source Code Analysis..."
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.AnalyzesView"
            icon="icons/16x16/analyzes_view.png"
            id="com.puresol.coding.client.AnalyzesView"
            name="Analyzes"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.AnalysisRunsView"
            icon="icons/16x16/analysis_runs_view.png"
            id="com.puresol.coding.client.AnalysisRunsView"
            name="Analysis Runs"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.AnalysisRunContentBrowserView"
            icon="icons/16x16/folders_explorer.png"
            id="com.puresol.coding.client.AnalysisRunContentBrowserView"
            name="Analysis Run Content Explorer"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.AnalysisReport"
            id="com.puresol.coding.client.AnalysisReport"
            name="Analysis Report"
            restorable="true">
      </view>
      <view
            category="org.eclipse.debug.ui"
            class="com.puresol.coding.client.views.OsgiDebug"
            id="com.puresol.coding.client.OsgiDebug"
            name="OSGI Debug"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.SupportedLanguages"
            id="com.puresol.coding.client.SupportedLanguages"
            name="Supported Languages"
            restorable="true">
      </view>
      <category
            id="com.puresol.coding.Analysis"
            name="Code Analysis">
      </category>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.ParserTreeView"
            id="com.puresol.coding.client.ParserTreeViewer"
            name="Parser Tree Viewer"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.GrammarViewer"
            id="com.puresol.coding.client.GrammarViewer"
            name="Grammar Viewer"
            restorable="true">
      </view>
      <view
            category="com.puresol.coding.Analysis"
            class="com.puresol.coding.client.views.AnalysisStoreView"
            id="com.puresol.coding.client.AnalysisStore"
            name="Analysis Store"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.puresol.coding.client.commands.NewAnalysis"
            id="com.puresol.ui.file.newAnalysis"
            name="newAnalysis">
      </command>
      <command
            defaultHandler="com.puresol.coding.client.commands.SwitchWorkspace"
            id="com.puresol.ui.file.switchWorkspace"
            name="switchWorkspace">
      </command>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.puresol.rcp.coding.client.plugin.application"
            name="Source Code Analysis">
         <property
               name="appName"
               value="Source Code Analysis">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.puresol.coding.client.CodeAnalysisPreferences"
            class="com.puresol.coding.client.preferences.NewAnalysisPreferencePage"
            id="com.puresol.coding.client.NewAnalysisPreferences"
            name="New Analysis">
      </page>
      <page
            class="com.puresol.coding.client.preferences.CodeAnalysisPreferencePage"
            id="com.puresol.coding.client.CodeAnalysisPreferences"
            name="Code Analysis">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.puresol.coding.client.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.puresol.coding.client.editors.FileAnalysisEditor"
            default="false"
            id="com.puresol.coding.client.FileAnalysisEditor"
            name="File Analysis">
      </editor>
      <editor
            class="com.puresol.coding.client.editors.DirectoryAnalysisEditor"
            default="false"
            id="com.puresol.coding.client.DirectoryAnalysisEditor"
            name="Directory Analysis">
      </editor>
      <editor
            class="com.puresol.coding.client.editors.NotAnalyzedEditor"
            default="false"
            id="com.puresol.coding.client.NotAnalyzedEditor"
            name="Not Analyzed">
      </editor>
   </extension>
   <extension
         id="SourceCodeClient"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.puresol.coding.client.application.plugin.application"
            name="Source Code Analysis">
         <property
               name="appName"
               value="Source Code Analysis">
         </property>
      </product>
   </extension>

</plugin>
